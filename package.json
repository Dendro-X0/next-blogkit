{
  "name": "blogkit",
  "version": "1.3.6",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx src/lib/db/migrate.ts",
    "db:seed": "tsx src/lib/db/seed.ts",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome lint .",
    "lint:biome": "biome lint .",
    "format": "biome format --write .",
    "check:biome": "biome check .",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "lhci:collect": "lhci collect --config=lighthouserc.js",
    "lhci:assert": "lhci assert --config=lighthouserc.js",
    "lhci": "lhci autorun --config=lighthouserc.js",
    "lhci:desktop": "lhci autorun --config=lighthouserc.desktop.js",
    "lhci:win": "if not exist .tmp mkdir .tmp && set TEMP=.tmp&& set TMP=.tmp&& lhci autorun --config=lighthouserc.js",
    "lhci:desktop:win": "if not exist .tmp mkdir .tmp && set TEMP=.tmp&& set TMP=.tmp&& lhci autorun --config=lighthouserc.desktop.js"
  },
